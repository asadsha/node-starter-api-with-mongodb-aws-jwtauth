{
	"env": {
		"node": true,
		"es6": true
	},
	"extends": ["airbnb-base", "prettier"],
	"plugins": ["prettier"],
	"rules": {
		"linebreak-style": ["off", "windows"],
		"quotes": ["error", "single"],
		"semi": ["error", "always"],
		"no-console": "off",
		"consistent-return": "off",
		"eqeqeq": "off"
	}
}
